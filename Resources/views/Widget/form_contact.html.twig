{% form_theme form with ['RohoCoreBundle:Form:fields.html.twig', 'RohoExtraFormBundle:Form:fields.html.twig'] %}
<div class="col-md-12">
    {% if status and message %}
        <div class="alert alert-{{status == 'success' ? 'success' : 'danger'}}">{{message}}</div>
    {% endif %}
    {{form(form)}}
</div>